.c_fff {
  color: #FFFFFF;
}
.fixed_t {
  padding: 0rpx 30rpx 16rpx;
  top: 0;
}
.fixed_t .out {
  height: 60rpx;
  line-height: 60rpx;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  background-image: url(data:image/png;base64,);
  background-size: 690rpx 60rpx;
  position: relative;
}
.fixed_t .out .redLine {
  position: absolute;
  height: 60rpx;
  line-height: 60rpx;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  width: 50%;
}
.fixed_t .out .txtLine {
  position: absolute;
  height: 60rpx;
  line-height: 60rpx;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  width: 100%;
}
.fixed_t .out .txtLine text {
  width: 25%;
}
.fixed_t .out .txtLine .txtLine {
  text-indent: -1em;
}

